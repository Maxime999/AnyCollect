							"Tags": {"cpuID": "$1"},
							"ComputeRate": true,
							"ConvertToUnitsPerSecond": true
						},
						{
							"Name": ["cpu", "core", "guest"],
							"Value": "$10",
							"Unit": "jiffies",
							"Tags": {"cpuID": "$1"},
							"ComputeRate": true,
							"ConvertToUnitsPerSecond": true
